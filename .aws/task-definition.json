{
  "containerDefinitions": [
    {
      "name": "dc-game",
      "image": "public.ecr.aws/v0g8b0z8/dc-game:latest",
      "cpu": 0,
      "portMappings": [
        {
          "name": "dc-game-3000-tcp",
          "containerPort": 3000,
          "hostPort": 3000,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true,
      "secrets": [
        {
          "name": "PORT",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:561166824006:secret:dc-env-0ClPYi:PORT::"
        }
      ]
    }
  ],
  "family": "dc-td-game",
  "executionRoleArn": "arn:aws:iam::561166824006:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "2048",
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  }
}
